###check if .xml annotations is correct

import cv2
import numpy as np
from matplotlib import pyplot as plt

img = cv2.imread(r'C:\Users\domin\OneDrive\Documents\29.png')
img_cvt=cv2.cvtColor(img, cv2.COLOR_BGR2RGB)
cv2.rectangle(img_cvt,(311 , 180 ), (444 , 311),(0,255,0),6)
plt.imshow(img_cvt)
plt.show()
